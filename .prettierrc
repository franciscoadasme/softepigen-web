{
    "plugins": [
        "@prettier/plugin-php",
        "prettier-plugin-blade",
        "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./resources/css/app.css",
    "phpVersion": "8.4",
    "singleQuote": true,
    "overrides": [
        {
            "files": ["*.blade.php"],
            "options": {
                "parser": "blade"
            }
        }
    ]
}
